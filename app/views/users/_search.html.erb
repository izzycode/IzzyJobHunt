    <div id='indeed_search_wrapper'>
      <script type='text/javascript'>
        function clearDefaults() {
          var formInputs = document.getElementById('indeed_jobform').elements;
          for(var i = 0; i < formInputs.length; i++) {
            if(formInputs[i].value == 'title, keywords' || formInputs[i].value == 'city, state, or zip') {
            formInputs[i].value = '';
            }
          }
        }
      </script>
      <!-- add modal to open external link internally -->
      <form onsubmit='clearDefaults();' method='get' action='http://www.indeed.com/jobs' id='indeed_jobform' target="_new" class="form-horizontal">
        <div id="qc"><label class="control-label">What:</label><input type='text' onfocus='this.value=""' placeholder='title, keywords' name='q' id='q' class="form-control"></div>
        <div id="lc"><label class="control-label">Where:</label><input type='text' onfocus='this.value=""' placeholder='city, state, or zip' name='l' id='l' class="form-control"></div>
        <div id='indeed_search_footer'>
        <div style='float:left'><input type='submit' value='Find Jobs' class='btn btn-sm btn-default'></div>
        </div>
        <input type='hidden' name='indpubnum' id='indpubnum' value='3380214054581298'>
      </form>
    </div>

