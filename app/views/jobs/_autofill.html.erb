<h2>Let's try to autofill</h2>

<%= form_for @job do |f| %>
  <%= f.label :web_address, class:"control-label" %>
  <%= f.text_field :web_address, class:"form-control", placeholder:"https://www.new-job-posting-path.com", data:{intro:"Just drag and drop/copy and paste the job posting's URL"}%><br />
  <%= f.submit "Let's give it a try", class:"btn btn-md btn-success"%>
<% end %>
<br />
